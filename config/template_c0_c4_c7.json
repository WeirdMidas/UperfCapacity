              "ac": "auto", 
              "pc": "auto"
            }, 
            {
              "k": ".", 
              "ac": "bg", 
              "pc": "auto"
            }
          ]
        }, 
        {
          "name": "SystemServer", 
          "regex": "system_server", 
          "pinned": true, 
          "rules": [

          "touch": 96, 
          "boost": 97
        }
      }, 
      "rules": [
        {
          "name": "Launcher", 
          "regex": "/HOME_PACKAGE/", 
          "pinned": true, 
          "rules": [
            {
              "k": "/MAIN_THREAD/", 
              "ac": "crit", 
              "pc": "rtusr"
            }, 
            {
              "k": "^(RenderThread|GLThread)", 
              "ac": "crit", 
              "pc": "rtusr"
            }, 
            {
              "k": "^(GPU completion|HWC release|hwui|FramePolicy|ScrollPolicy|ged-swd)", 
              "ac": "bg", 
              "pc": "rtusr"
            }, 
            {
              "k": ".", 
              "ac": "auto", 
              "pc": "auto"
            }
          ]
        }, 
        {
          "name": "SurfaceFlinger", 
          "regex": "/system/bin/surfaceflinger", 
          "pinned": true, 
          "rules": [
            {
              "k": "/MAIN_THREAD/", 
              "ac": "crit", 
              "pc": "auto"
            }, 
            {
              "k": "^(app|RenderEngine)", 
              "ac": "crit", 
              "pc": "auto"
            }, 
            {

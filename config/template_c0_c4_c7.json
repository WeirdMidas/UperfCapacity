{
  "meta": {
    "name": "platform[yy.mm.dd]", 
    "author": "author@community"
  }, 
  "modules": {
    "switcher": {
      "switchInode": "/sdcard/Android/yc/uperf/cur_powermode.txt", 
      "perapp": "/sdcard/Android/yc/uperf/perapp_powermode.txt", 
      "hintDuration": {
        "idle": 0.0, 
        "touch": 4.0, 
        "trigger": 0.03, 
        "gesture": 0.1, 
        "switch": 0.4, 
        "junk": 0.06
      }
    }, 
    "atrace": {
      "enable": false
    }, 
    "log": {
      "level": "info"
    }, 
    "input": {
      "enable": true, 
      "swipeThd": 0.03, 
      "gestureThdX": 0.03, 
      "gestureThdY": 0.03, 
      "gestureDelayTime": 2.0, 
      "holdEnterTime": 1.0
    }, 
    "sfanalysis": {
      "enable": true, 
      "renderIdleSlackTime": 0.2
    }, 
    "cpu": {
      "enable": true, 
      "powerModel": [
    "cpu": {
      "enable": true, 
      "powerModel": [
        {
          "efficiency": 100, 
          "nr": 4, 
          "typicalPower": 0.0, 
          "typicalFreq": 0.0, 
          "sweetFreq": 0.0, 
          "plainFreq": 0.0, 
